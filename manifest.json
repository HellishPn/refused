{
  "name": "Refused",
  "description": "Block ads on your favorite persian websites.",
  "version": "0.2.9",
  "manifest_version": 2,
  "icons": {
    "48": "images/48.png",
    "96": "images/96.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "options_ui": {
    "page": "options.html"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "storage",
    "unlimitedStorage",
    "<all_urls>"
  ],
  "default_locale": "en"
}
